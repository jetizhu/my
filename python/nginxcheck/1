python check.py  |jq '.[] | {server_name:.server_name,server_port:.port,backend:.backend[]}' |jq '. | {server_name:.server_name,server_port:.server_port,backend_ip:.backend.backend_ip,backend_path:.backend.backend_path}'
cat test.json |tr -d '\n' |tr '}' '\n' |sed 's/$/}/g' >2.json


